{
  "name": "contacts-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon -w src -w .env -e ts,json --exec node -r ts-node/register ./src/server.ts",
    "dev:debug": "nodemon -V -w src -w .env -e ts,json --exec node --inspect=0.0.0.0 -r ts-node/register ./src/server.ts",
    "prebuild": "rimraf dist/*",
    "build": "webpack",
    "knex": "knex --knexfile knexfile.ts",
    "db:up": "npm run knex migrate:latest && npm run knex seed:run",
    "db:down": "./scripts/destroy-db.js",
    "db:refresh": "npm run db:down && npm run db:up",
    "avatars": "ts-node ./scripts/get-avatars.ts",
    "setup": "npm run db:up && npm run avatars",
    "start": "node dist/server.js",
    "lint": "biome lint .",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "fs-extra": "^11.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "objection": "^3.1.4",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/rimraf": "^3.0.2",
    "@types/sqlite3": "^3.1.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [],
  "author": "Andy Wright <atwright147@gmail.com>",
  "license": "MIT"
}
