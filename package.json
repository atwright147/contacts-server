{
  "name": "contacts-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon -w src -w .env -e ts,json --exec node -r ts-node/register ./src/server.ts",
    "dev:debug": "nodemon -V -w src -w .env -e ts,json --exec node --inspect=0.0.0.0 -r ts-node/register ./src/server.ts",
    "knex": "knex --knexfile knexfile.ts",
    "db:up": "npm run knex migrate:latest && npm run knex seed:run",
    "db:down": "./scripts/destroy-db.js",
    "db:refresh": "npm run db:down && npm run db:up",
    "avatars": "ts-node ./scripts/get-avatars.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@vscode/sqlite3": "^5.0.7",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "objection": "^3.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/rimraf": "^3.0.2",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "keywords": [],
  "author": "Andy Wright <atwright147@gmail.com>",
  "license": "MIT"
}
