{
  "name": "contacts-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon -w src -w .env -e ts,json --exec node -r ts-node/register ./src/server.ts",
    "dev:debug": "nodemon -V -w src -w .env -e ts,json --exec node --inspect=0.0.0.0 -r ts-node/register ./src/server.ts",
    "knex": "knex --knexfile knexfile.ts",
    "db:up": "npm run knex migrate:latest && npm run knex seed:run",
    "db:down": "./scripts/destroy-db.js",
    "db:refresh": "npm run db:down && npm run db:up",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "knex": "^0.95.11",
    "objection": "^2.2.17",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/rimraf": "^3.0.2",
    "@types/sqlite3": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "keywords": [],
  "author": "Andy Wright <atwright147@gmail.com>",
  "license": "MIT"
}
